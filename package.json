{
  "name": "@motionpicture/sskts-factory",
  "version": "1.0.0-alpha.3",
  "description": "sskts factory library",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "directories": {
    "doc": "./doc",
    "lib": "./lib",
    "example": "./example",
    "test": "./test"
  },
  "scripts": {
    "build": "tsc",
    "check": "npm run tslint && nsp check && npm run snyk",
    "clean": "rimraf test/**/*.js ./lib npm-debug.log* ./doc",
    "doc": "node_modules/.bin/jsdoc -c jsdoc.json",
    "nyc": "nyc mocha",
    "snyk": "snyk test --ignore-policy",
    "test": "npm run check",
    "tslint": "tslint --type-check --project tsconfig.json -c tslint.json src/**/*.ts"
  },
  "author": {
    "name": "Motionpicture co.,ltd."
  },
  "contributors": [
    {
      "name": "Tetsu Yamazaki",
      "email": "yamazaki@motionpicture.jp"
    }
  ],
  "license": "UNLICENSED",
  "nyc": {
    "check-coverage": true,
    "include": [
      "lib/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/mocha": "^2.2.43",
    "@types/underscore": "^1.8.3",
    "@types/validator": "^6.2.3",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.5.4",
    "mocha": "^3.5.2",
    "nsp": "^2.8.0",
    "nyc": "^11.2.1",
    "rimraf": "^2.6.1",
    "snyk": "^1.40.2",
    "tslint": "^5.7.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "^2.5.2"
  },
  "dependencies": {
    "@motionpicture/coa-service": "^4.0.1",
    "@motionpicture/gmo-service": "^4.0.0",
    "debug": "^3.0.1",
    "moment": "^2.18.1",
    "underscore": "^1.8.3",
    "validator": "^8.2.0"
  }
}
